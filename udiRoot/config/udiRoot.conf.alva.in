nodeContextPrefix=
udiMount=/var/udiMount
loopMount=/var/loopUdiMount
imagePath=/scratch1/scratchdirs/craydock/docker
udiRootPath=@@@PREFIX@@@
sitePreMountHook=@@@CONFIG_DIR@@@/premount.sh
sitePostMountHook=@@@CONFIG_DIR@@@/postmount.sh
siteFs=/scratch1 \
       /global/u1 /global/u2 /global/syscom /global/common \
       /global/scratch2 /global/project /global/projectb \
       /global/dna /var/opt/cray/alps/spool \
       /var/spool/slurmd
optUdiImage=@@@PREFIX@@@/sshd
etcPath=@@@PREFIX@@@/etc_files
kmodBasePath=@@@PREFIX@@@/kmod
kmodCacheFile=/tmp/udiRootLoadedModules.txt
batchType=slurm
system=@@@SYSTEM@@@
