imagePath=/scratch1/scratchdirs/craydock/docker
modprobePath=/sbin/modprobe
insmodPath=/sbin/insmod
cpPath=/bin/cp
mvPath=/bin/mv
chmodPath=/bin/chmod
rootfsType=rootfs
udiRootPath=@@@PREFIX@@@
